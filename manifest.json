{
    "name": "Specimen",
    "version": "1.0",
    "description": "A Chrome Bookmarks Management Plugin",
    "permissions": ["bookmarks"],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/bookmark_icon_16.png",
            "32": "images/bookmark_icon_32.png",
            "48": "images/bookmark_icon_48.png",
            "128": "images/bookmark_icon_128.png"
        }
    },
    "icons": {
        "16": "images/bookmark_icon_16.png",
        "32": "images/bookmark_icon_32.png",
        "48": "images/bookmark_icon_48.png",
        "128": "images/bookmark_icon_128.png"
    },
    "manifest_version": 2
}