{
    "name": "Bookmarks Manager",
    "version": "1.0",
    "description": "Chrome Bookmarks Management Tools",
    "permissions": ["storage", "bookmarks"],
    "background": {
        "scripts": ["init.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/bookmark_icon_16.png",
            "32": "images/bookmark_icon_32.png",
            "48": "images/bookmark_icon_48.png",
            "128": "images/bookmark_icon_128.png"
        }
    },
    "icons": {
        "16": "images/bookmark_icon_16.png",
        "32": "images/bookmark_icon_32.png",
        "48": "images/bookmark_icon_48.png",
        "128": "images/bookmark_icon_128.png"
    },
    "manifest_version": 2
}